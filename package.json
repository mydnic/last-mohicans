        "laravel-mix": "^5.0",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "resolve-url-loader": "3.1.1",
        "sass": "^1.24.4",
        "sass-loader": "8.*",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "vue-router": "^3.1.4"
    }
